{
  "name": "page-v4",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:dev": "ng serve --configuration=dev --open",
    "start:qa": "ng serve --configuration=qa  --open",
    "start:prod": "ng serve --configuration=prod",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": " npx tslint src/**/*.ts --fix && ng lint",
    "lint:styles": "stylelint \"src/**/*.scss\" --fix",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "cd dist/ && node server",
    "build:ssr": "rm -rf dist/* && npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod && ng run page-v4:server:production --bundleDependencies all",
    "publish:ssr": "cd .. && rm -rf office-list-web-build/* && cp -r page-v4/dist/* office-list-web-build/",
    "publish": "npm run build:ssr && npm run publish:ssr && npm run copy",
    "copy": "npm run copy:seo && npm run copy:images && npm run copy:images:mailings && npm run copy:images:misc && npm run copy:images:emails && npm run copy:images:invoiceemail && npm run copy:ecosystem",
    "copy:seo": "cd .. && cp -r office-list-web-build/browser/assets/seo/* office-list-web-build/browser/ && rm -rf office-list-web-build/browser/assets/seo/",
    "copy:images": "cd .. && mkdir office-list-web-build/browser/images/ && cp -r page-v4/old_site/images/* office-list-web-build/browser/images/ ",
    "copy:images:mailings": "cd .. && mkdir office-list-web-build/browser/assets/images/mailings/ && cp -r page-v4/old_site/mailings/* office-list-web-build/browser/assets/images/mailings/",
    "copy:images:misc": "cd .. && mkdir office-list-web-build/browser/assets/images/misc/ && cp -r page-v4/old_site/misc/* office-list-web-build/browser/assets/images/misc/",
    "copy:images:emails": "cd .. && mkdir office-list-web-build/browser/images/emails/ && cp -r page-v4/old_site/emails/* office-list-web-build/browser/images/emails/",
    "copy:images:invoiceemail": "cd .. && mkdir office-list-web-build/browser/images/invoiceemail/ && cp -r page-v4/old_site/invoiceemail/* office-list-web-build/browser/images/invoiceemail/",
    "copy:ecosystem": "cd .. && cp page-v4/ecosystem.config.js office-list-web-build/"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.1.0",
    "@agm/js-marker-clusterer": "^1.1.0",
    "@agm/snazzy-info-window": "^1.1.0",
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@auth0/angular-jwt": "^3.0.1",
    "@babel/polyfill": "^7.7.0",
    "@google/markerclustererplus": "^3.0.1",
    "@ng-select/ng-select": "^3.7.0",
    "@ng-toolkit/universal": "^8.0.3",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "angular-ng-autocomplete": "^1.1.14",
    "classlist.js": "^1.1.20150312",
    "express": "^4.15.2",
    "helmet": "^3.21.2",
    "js-marker-clusterer": "^1.0.0",
    "lottie-web": "^5.5.10",
    "ng-lazyload-image": "^7.0.1",
    "ngx-cacheable": "^1.3.2",
    "ngx-cookie-service": "^2.2.0",
    "ngx-device-detector": "^1.3.20",
    "ngx-lottie": "^5.3.2",
    "ngx-mask": "^8.1.6",
    "ngx-spinner": "^8.1.0",
    "ngx-useful-swiper": "^8.0.1",
    "ngx-webstorage-service": "^4.1.0",
    "rxjs": "^6.5.3",
    "snazzy-info-window": "^1.1.1",
    "swiper": "^5.2.1",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "webpack-cli": "^3.3.10",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.21",
    "@angular/cli": "^8.3.21",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^3.5.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.21",
    "@types/swiper": "^4.4.8",
    "codelyzer": "^5.2.1",
    "husky": "^3.1.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "^5.4.2",
    "stylelint": "^11.1.1",
    "ts-loader": "^5.2.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.1.0"
  }
}
